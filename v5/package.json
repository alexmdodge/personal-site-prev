{
  "name": "personal-site",
  "description": "My personal web app containing my portfolio and blog.",
  "version": "1.0.0",
  "repository": {
    "type": "webapp",
    "url": "https://github.com/alexmdodge/personal-site"
  },
  "license": "MIT",
  "scripts": {
    "build:styles": "sass assets/styles/styles.scss assets/styles/styles.css",
    "watch:styles": "sass --watch assets/styles:assets/styles",
    "start": "concurrently \"npm run watch:styles\" \"au run --watch\"",
    "build:prod": "npm run build:styles && au build --env prod && gulp build"
